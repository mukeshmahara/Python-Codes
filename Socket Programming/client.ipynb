    "import socket\n",
    "HOST= '127.0.0.1'\n",
    "PORT=65432\n",
    "with socket.socket(socket.AF_INET, socket.SOCK_STREAM) as s:\n",
    "    s.connect((HOST,PORT))\n",
    "    s.sendall(b'Helo=lo, World')\n",
